<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/detect"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.trashdetective.DetectedClassResult">

    <Button
        android:id="@+id/rezero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="돌아가기/확인"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.688" />

    <TextView
        android:id="@+id/overlaptextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="overlap result"
        app:layout_constraintBottom_toTopOf="@+id/rezero"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.508" />

    <TextView
        android:id="@+id/noOverlaptextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="no overlap result"
        app:layout_constraintBottom_toTopOf="@+id/rezero"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/overlaptextview"
        app:layout_constraintVertical_bias="0.514" />
</androidx.constraintlayout.widget.ConstraintLayout>